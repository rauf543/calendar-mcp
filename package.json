{
  "name": "calendar-mcp",
  "version": "1.0.0",
  "description": "MCP server for unified calendar management across Google, Microsoft 365, and Exchange",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "calendar-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "calendar",
    "google-calendar",
    "microsoft-graph",
    "exchange",
    "ews",
    "claude",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@azure/msal-node": "^2.16.2",
    "@ewsjs/xhr": "^3.1.3",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@vercel/mcp-adapter": "^1.0.0",
    "dotenv": "^16.4.7",
    "ews-javascript-api": "^0.12.0",
    "google-auth-library": "^9.15.1",
    "googleapis": "^144.0.0",
    "httpntlm": "^1.8.13",
    "luxon": "^3.5.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.24.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vercel/node": "^5.5.15",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
